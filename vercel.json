{
  "version": 2,
  "name": "prompt-scribe-api",
  "builds": [
    {
      "src": "src/api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/main.py"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_ANON_KEY": "@supabase_anon_key",
    "CACHE_STRATEGY": "memory",
    "REDIS_ENABLED": "false",
    "LOG_LEVEL": "INFO"
  },
  "functions": {
    "src/api/main.py": {
      "maxDuration": 30,
      "memory": 512,
      "runtime": "python3.9"
    }
  },
  "regions": ["all"],
  "github": {
    "autoAlias": false
  }
}
