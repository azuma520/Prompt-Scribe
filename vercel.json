{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "CACHE_STRATEGY": "memory",
    "REDIS_ENABLED": "false",
    "LOG_LEVEL": "INFO"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "github": {
    "autoAlias": false
  }
}
